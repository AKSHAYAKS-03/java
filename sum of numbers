package java1;
import java.util.*;
public class java1
{
    public static void main(String[] args)
    {
        int sum=0,i;
        for(i=100;i<=200;i++)
        {
            if(i%7==0)
            {
               sum+=i;              
            }
        }
        System.out.println("Sum="+sum);
     }    
}
